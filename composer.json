{
    "name": "percipiolondon/craft-timeloop",
    "description": "This is a plugin to make repeating dates",
    "type": "craft-plugin",
    "version": "0.1.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "timeloop"
    ],
    "support": {
        "docs": "https://github.com/percipiolondon/timeloop/blob/master/README.md",
        "issues": "https://github.com/percipiolondon/timeloop/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Percipio.London",
            "homepage": "https://percipio.london"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "require-dev": {
        "craftcms/cms": "^3.2.0",
        "codeception/codeception": "^3.0",
        "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "percipiolondon\\timeloop\\": "src/"
        }
    },
    "extra": {
        "name": "Timeloop",
        "handle": "timeloop",
        "developer": "percipiolondon",
        "developerUrl": "https://percipio.london",
        "documentationUrl": "https://github.com/percipiolondon/timeloop/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/percipiolondon/timeloop/master/CHANGELOG.md",
        "class": "percipiolondon\\timeloop\\Timeloop"
    }
}
